<Application x:Class="SatisfactoryCalculator.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:main="clr-namespace:SatisfactoryCalculator"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Resources/MainResourceDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            
            <main:MainWindowViewModel x:Key="MainWindowViewModel"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
